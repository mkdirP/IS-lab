<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.web.mapper.ImportLogMapper">

    <!-- 插入日志 -->
    <insert id="insertImportLog">
        INSERT INTO import_logs (file_name, file_url, status)
        VALUES (#{fileName}, #{fileUrl}, #{status});
    </insert>

</mapper>
